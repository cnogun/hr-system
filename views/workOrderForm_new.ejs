                        <div class="form-group">
                            <label>현재원</label>
                            <input type="number" name="personnel.current" class="form-control" required>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group" style="width: 100%;">
                            <label>사고내용</label>
                            <textarea name="personnel.accidentDetails" class="form-control" rows="3" placeholder="사고내용을 입력하세요"></textarea>
